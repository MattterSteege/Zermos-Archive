@model string

@if (Model == "success")
{
<script>
    new ZermosModal()
    .addHeading({text: "Gelukt!"})
    .addText({text: "Je hebt succesvol de koppeling gelegd met Somtoday, je kan nu gebruik maken van de Somtoday functionaliteiten op Zermos."})
    .openTroughAppending(main);

    newSidebar();
</script>
}
else if (Model == "failed")
{

<script>
    new ZermosModal()
    .addHeading({text: "Helaas"})
    .addText({text: "het is niet gelukt om de koppeling te leggen met Somtoday, probeer het nog een keer."})
    .openTroughAppending(main);
</script>
}