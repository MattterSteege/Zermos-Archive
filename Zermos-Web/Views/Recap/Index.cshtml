    const colorPallets = 4;
    
    FullScreenMain();
    main.requestFullscreen();
    //reverse the slide order so the first slide is shown first
    const slides = document.querySelectorAll('.slide');
    slides.forEach((slide, index) => {
        slide.style.zIndex = index;
        slide.style.opacity = 0;
        slide.classList.add(`color-${Math.floor(Math.random() * colorPallets) + 1}`);
    });

    window.location.hash = '#/Recap';
    
    var currentSlide = 1;    
    function nextSlide()
    {
        currentSlide++;
        if(currentSlide >= document.querySelectorAll('.slide').length - 1)
        {
            currentSlide = document.querySelectorAll('.slide').length;
            document.querySelector('.next').style.display = 'none';
        }
        else
        {
            document.querySelector('.next').style.display = '';
            document.querySelector('.prev').style.display = '';
            
        }
        showSlide(currentSlide);
    }
    
    function previousSlide()
    {
    
        currentSlide--;
        if(currentSlide <= 0)
        {
            currentSlide = 0;
            document.querySelector('.prev').style.display = 'none';
        }
        else
        {
            document.querySelector('.prev').style.display = '';
            document.querySelector('.next').style.display = '';
        }
        showSlide(currentSlide - 1);
    }
    
    function showSlide(index)
    {
        const slides = document.querySelectorAll('.slide');
        slides.forEach((slide, i) => {
            //dont display block/none only z-index
            if(i === index - 1)
            {
                slide.style.opacity = 1;
            }
            else
            {
                slide.style.opacity = 0;
            }
        });
    }
    
    showSlide(1);
    
    document.addEventListener('keydown', (e) => {
        if(e.key == 'ArrowRight')
        {
            nextSlide();
        }
        else if(e.key == 'ArrowLeft')
        {
            previousSlide();
        }
    });
    
    //if swipe to next slide
    let touchstartX = 0;
    let touchendX = 0;
    const sliderC = document.querySelector('.slide-container');
    sliderC.addEventListener('touchstart', (e) => {
        touchstartX = e.changedTouches[0].screenX;
    });

    sliderC.addEventListener('touchend', (e) => {
        touchendX = e.changedTouches[0].screenX;
        handleGesture();
    });
    
    function handleGesture() {
        //if traveled more than 100px
        if (touchendX < touchstartX) {
            if(touchstartX - touchendX > 100)
            {
                nextSlide();
            }
        }
        if (touchendX > touchstartX) {
            if(touchendX - touchstartX > 100)
            {
                previousSlide();
            }
        }
        
    }
    
    //get all the .background elements
    const backgrounds = document.querySelectorAll('.background');
    //loop through the backgrounds
    backgrounds.forEach((background) => {
        //for the first svg pick one from the first layer
        const layer1 = backgroundItems.Layer1[Math.floor(Math.random() * backgroundItems.Layer1.length)];
        //for the second svg pick one from the second layer that is not the same as the first
        let layer2 = backgroundItems.Layer2[Math.floor(Math.random() * backgroundItems.Layer2.length)];
        while(layer2 === layer1)
        {
            layer2 = backgroundItems.Layer2[Math.floor(Math.random() * backgroundItems.Layer2.length)];
        }
        //for the third svg pick one from the third layer that is not the same as the first or second
        let layer3 = backgroundItems.Layer3[Math.floor(Math.random() * backgroundItems.Layer3.length)];
        while(layer3 === layer1 || layer3 === layer2)
        {
            layer3 = backgroundItems.Layer3[Math.floor(Math.random() * backgroundItems.Layer3.length)];
        }
        //set the background images
        background.children[2].outerHTML = `<div class="layer-1">${layer1}</div>`;
        background.children[1].outerHTML = `<div class="layer-2">${layer2}</div>`;
        background.children[0].outerHTML = `<div class="layer-3">${layer3}${layer3}</div>`;
    });
